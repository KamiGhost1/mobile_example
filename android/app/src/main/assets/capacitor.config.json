{
	"appId": "com.suai.pwa",
	"appName": "Burlin Roman app",
	"webDir": "dist",
	"bundledWebRuntime": false,
	"backgroundColor": "#282D46",
	"plugins": {
		"LocalNotifications": {
			"smallIcon": "logo.png",
			"iconColor": "#000000",
			"sound": "beep.wav"
		},
		"SplashScreen": {
			"launchShowDuration": 0
		},
		"PushNotifications": {
			"presentationOptions": [
				"badge",
				"sound",
				"alert"
			]
		}
	},
	"server": {
		"hostname": "localhost",
		"androidScheme": "http",
		"iosScheme": "http"
	}
}
